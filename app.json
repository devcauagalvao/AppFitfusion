    "name": "FitFusion",
    "slug": "projetoepa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-verde.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
   "splash": {
  "image": "./assets/images/logo-verde.png",
  "resizeMode": "contain",
  "backgroundColor": "#050505"
},
